{
  "database": {
    "rules": "database-security.json"
  },
  "hosting": {
    "site":"v34l",
    "public": "src",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "src/index.html"
      }
    ]
  }
}
