{
  "database": {
    "rules": "database-security.json"
  },
  "hosting": [
    {
      "site": "v34l",
      "public": "src",
      "ignore": [
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "V34L",
      "source": ".",
      "frameworksBackend": {}
    }
  ]
}